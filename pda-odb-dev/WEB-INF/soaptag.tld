<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.1//EN" "http://java.sun.com/j2ee/dtds/web-jsptaglibrary_1_1.dtd">

<taglib>
<tlibversion>1.6</tlibversion>
<jspversion>1.1</jspversion>
<shortname>SoapTag</shortname>
<uri>http://www.servletsuite.com/servlets/soaptag</uri>
<info>SOAP request tag</info>

<tag>
<name>SOAPrequest</name>
<tagclass>com.cj.soap.doSOAP</tagclass>
<teiclass>com.cj.soap.strVariable</teiclass>
<bodycontent>JSP</bodycontent>
<info>SOAP request</info>

<attribute>
<name>url</name>
<required>true</required>
<rtexprvalue>true</rtexprvalue>
</attribute>

<attribute>
<name>action</name>
<required>false</required>
<rtexprvalue>true</rtexprvalue>
</attribute>

<attribute>
<name>method</name>
<required>false</required>
<rtexprvalue>true</rtexprvalue>
</attribute>


<attribute>
<name>asynch</name>
<required>false</required>
<rtexprvalue>true</rtexprvalue>
</attribute>

<attribute>
<name>id</name>
<required>false</required>
<rtexprvalue>true</rtexprvalue>
</attribute>

</tag>

<tag>
<name>SOAPmessage</name>
<tagclass>com.cj.soap.SOAPmessage</tagclass>
<bodycontent>JSP</bodycontent>
<info>SOAP message</info>
</tag>

<tag>
<name>SOAPbody</name>
<tagclass>com.cj.soap.SOAPbody</tagclass>
<bodycontent>JSP</bodycontent>
<info>body for SOAP message</info>
</tag>

<tag>
<name>SOAPheaders</name>
<tagclass>com.cj.soap.SOAPheaders</tagclass>
<bodycontent>JSP</bodycontent>
<info>headers for SOAP message</info>

<attribute>
<name>uri</name>
<required>true</required>
<rtexprvalue>true</rtexprvalue>
</attribute>
</tag>

<tag>
<name>SOAPheader</name>
<tagclass>com.cj.soap.SOAPheader</tagclass>
<bodycontent>JSP</bodycontent>
<info>header for SOAP message</info>

<attribute>
<name>name</name>
<required>true</required>
<rtexprvalue>true</rtexprvalue>
</attribute>

<attribute>
<name>mandatory</name>
<required>false</required>
<rtexprvalue>true</rtexprvalue>
</attribute>

</tag>

</taglib>

